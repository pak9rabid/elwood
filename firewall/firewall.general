# This file contains general firewall rules

# Clear all rules and set default policies
$SETPOLICY INPUT DROP
$FLUSHCHAIN INPUT

$SETPOLICY OUTPUT ACCEPT
$FLUSHCHAIN OUTPUT

$SETPOLICY FORWARD DROP
$FLUSHCHAIN FORWARD

$SETPOLICY PREROUTING ACCEPT
$FLUSHCHAIN PREROUTING

$SETPOLICY POSTROUTING ACCEPT
$FLUSHCHAIN POSTROUTING

echo -e "Completed firewall.general"
